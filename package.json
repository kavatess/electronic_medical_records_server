{
  "name": "nestjs-starter",
  "version": "1.0.0",
  "description": "NestJs Starter with mongoose, rabbitmq",
  "license": "UNLICENSED",
  "scripts": {
    "build:docker": "docker build --rm -t nestjs-starter . --build-arg GIT_COMMIT=$GIT_COMMIT --build-arg GIT_BRANCH=$GIT_BRANCH --build-arg GIT_TOKEN=$GIT_TOKEN",
    "build": "nest build",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts --fix src",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "start:debug": "nest start --debug --watch",
    "start:infra": "docker-compose -f docker-compose.yml up -d ",
    "start": "node dist/main",
    "test": "docker-compose -f test/docker-compose.yml up --build --abort-on-container-exit",
    "tool:generate": "ts-node ./tools/module-generator/index.ts"
  },
  "dependencies": {
    "@casl/ability": "5.3.1",
    "@golevelup/nestjs-rabbitmq": "1.18.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "8.1.2",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "8.1.2",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/microservices": "8.1.2",
    "@nestjs/mongoose": "8.0.1",
    "@nestjs/platform-express": "8.1.2",
    "@nestjs/swagger": "5.1.4",
    "@nestjs/terminus": "8.0.1",
    "bson": "^4.5.4",
    "cache-manager": "3.4.3",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "flat": "5.0.2",
    "json-extra": "2.0.1",
    "json2csv": "5.0.6",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "mongoose": "5.13.12",
    "mongoose-autopopulate": "0.14.0",
    "nestjs-pino": "1.4.0",
    "pino-pretty": "4.8.0",
    "randomstring": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.1.0",
    "swagger-ui-express": "4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@types/cache-manager": "3.4.0",
    "@types/express": "4.17.12",
    "@types/lodash": "4.14.170",
    "@types/mongoose": "5.7.36",
    "@types/node": "14.17.3",
    "@types/randomstring": "^1.1.7",
    "@types/uuid": "^8.3.1",
    "@types/ws": "7.4.4",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "copy": "0.3.2",
    "eslint": "7.28.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.23.4",
    "husky": "6.0.0",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.3.2"
  }
}
